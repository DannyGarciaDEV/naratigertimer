{
  "name": "photoism-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "npm install && node server.js",
    "dev": "concurrently \"nodemon server.js\" \"npm run start-client\"",
    "start-client": "cd photoism-app && npm start",
    "build-client": "cd photoism-app && npm install && npm run build"
  },
  "dependencies": {
    "express": "^4.21.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "concurrently": "^8.2.0"
  }
}
